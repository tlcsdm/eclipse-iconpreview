{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
    ':semanticCommits',
    ':label(dependencies)',
    'schedule:weekly',
  ],
  enabledManagers: [
    'maven',
  ],
  maven: {
    managerFilePatterns: [
      '/^pom\\.xml$/',
      '/^.*\\/pom\\.xml$/',
    ],
  },
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/^.*\\.target$/',
      ],
      matchStrings: [
        '<repository location="https://mirrors\\.aliyun\\.com/eclipse/releases/(?<depName>[^/]+)/(?<currentValue>[^/]+)/"',
      ],
      depNameTemplate: 'eclipse-release',
      versioningTemplate: 'loose',
      datasourceTemplate: 'maven',
      packageNameTemplate: 'eclipse/releases',
    },
  ],
  commitMessagePrefix: 'chore(deps):',
  dependencyDashboard: true,
  timezone: 'Asia/Shanghai',
}
